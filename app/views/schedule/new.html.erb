<div class="contents">
  <div class="container">
    <%= form_with model: @schedule, url: schedule_index_path do |form| %>
      <%= form.text_field :title %>
      <%= form.datetime_local_field :start %>
      <%= form.datetime_local_field :end %>
      <%= form.check_box :allday %>
      <%= form.label :allday, "終日" %>
      <!--<= form.text_area :message, size: "70x5" %>-->

      <%= form.submit "スケジュール登録"%>
    <% end %>
  </div>
</div>